{
	"name": "@ctx-core/theme",
	"version": "4.0.6",
	"description": "ctx-core theme support",
	"keywords": [
		"ctx-core",
		"time"
	],
	"homepage": "https://github.com/ctx-core/theme#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/theme/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/theme/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/function": "workspace:^18.1.0",
		"@ctx-core/html": "workspace:^9.0.0",
		"@ctx-core/object": "workspace:^19.0.3",
		"@ctx-core/store": "workspace:^25.1.2",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^5.0.3",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
