{
	"name": "@ctx-core/theme",
	"version": "6.0.45",
	"description": "ctx-core theme support",
	"keywords": [
		"ctx-core",
		"time"
	],
	"homepage": "https://github.com/ctx-core/theme#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/theme/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/theme/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc"
	},
	"dependencies": {
		"@ctx-core/function": "workspace:^20.2.4",
		"@ctx-core/html": "workspace:^11.0.20",
		"@ctx-core/object": "workspace:^21.0.23",
		"@ctx-core/store": "workspace:^27.0.37",
		"svelte": "3.44.1"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.1",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.108",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}