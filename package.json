{
	"name": "@ctx-core/theme",
	"version": "10.0.3",
	"description": "ctx-core theme support",
	"keywords": [
		"ctx-core",
		"time"
	],
	"homepage": "https://github.com/ctx-core/theme#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/theme/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/theme.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"invert_theme",
		"invert_theme_class"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/function": "^21.22.14",
		"@ctx-core/html": "^11.6.18",
		"@ctx-core/nanostores": "^9.0.3",
		"@ctx-core/object": "^31.1.0"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.4",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}